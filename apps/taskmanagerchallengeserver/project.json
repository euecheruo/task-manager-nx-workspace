{
  "name": "taskmanagerchallengeserver",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/taskmanagerchallengeserver/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": [ "--node-env=production" ],
        "cwd": "apps/taskmanagerchallengeserver"
      },
      "configurations": {
        "development": {
          "args": [ "--node-env=development" ]
        }
      }
    },
    "prune-lockfile": {
      "dependsOn": [ "build" ],
      "cache": true,
      "executor": "@nx/js:prune-lockfile",
      "outputs": [
        "{workspaceRoot}/dist/apps/taskmanagerchallengeserver/package.json",
        "{workspaceRoot}/dist/apps/taskmanagerchallengeserver/package-lock.json"
      ],
      "options": {
        "buildTarget": "build"
      }
    },
    "copy-workspace-modules": {
      "dependsOn": [ "build" ],
      "cache": true,
      "outputs": [
        "{workspaceRoot}/dist/apps/taskmanagerchallengeserver/workspace_modules"
      ],
      "executor": "@nx/js:copy-workspace-modules",
      "options": {
        "buildTarget": "build"
      }
    },
    "prune": {
      "dependsOn": [ "prune-lockfile", "copy-workspace-modules" ],
      "executor": "nx:noop"
    },
    "serve": {
      "continuous": true,
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": [ "build" ],
      "options": {
        "buildTarget": "taskmanagerchallengeserver:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "taskmanagerchallengeserver:build:development"
        },
        "production": {
          "buildTarget": "taskmanagerchallengeserver:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [ "{workspaceRoot}/coverage/{projectRoot}" ],
      "options": {
        "jestConfig": "apps/taskmanagerchallengeserver/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "migration": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "ts-node -r tsconfig-paths/register --project apps/taskmanagerchallengeserver/tsconfig.app.json apps/taskmanagerchallengeserver/src/seeder/tasks/seeder.task.ts",
        "args": []
      },
      "configurations": {
        "development": {
          "env": {
            "NODE_ENV": "development"
          }
        },
        "production": {
          "env": {
            "NODE_ENV": "production"
          }
        }
      }
    }
  }
}
